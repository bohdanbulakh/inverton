{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2021",
    "esModuleInterop": true,
    "outDir": "dist",
    "baseUrl": ".",
    "declaration": true,
    "sourceMap": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "preserveSymlinks": true,
  },
  "include": ["src"]
}
