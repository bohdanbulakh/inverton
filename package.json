{
  "name": "inverton",
  "version": "0.0.0",
  "license": "MIT",
  "bin": "dist/cli.js",
  "type": "commonjs",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "yarn build && node dist/index.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "ink": "^4.1.0",
    "ioredis": "^5.8.1",
    "meow": "^11.0.0",
    "react": "^18.2.0",
    "tsconfig-paths": "^4.2.0",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^3.0.1",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/eslint": "^9.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.0.32",
    "@types/unzipper": "^0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vdemedes/prettier-config": "^2.0.1",
    "ava": "^5.2.0",
    "chalk": "^5.2.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-config-xo-react": "^0.27.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "ink-testing-library": "^3.0.0",
    "jest": "^30.2.0",
    "prettier": "^2.8.7",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "xo": "^0.53.1"
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "xo": {
    "extends": "xo-react",
    "prettier": true,
    "rules": {
      "react/prop-types": "off"
    }
  },
  "prettier": "@vdemedes/prettier-config",
  "packageManager": "yarn@4.10.3"
}
